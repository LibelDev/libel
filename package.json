  "scripts": {
    "clean": "rm -rf dist",
    "dev": "NODE_ENV=development webpack serve --config ./config/webpack/webpack.config.dev.ts",
    "dev:egg": "DEBUG_EGG=true pnpm dev",
    "prebuild": "pnpm clean",
    "build": "pnpm prebuild && pnpm type:scss && NODE_ENV=production webpack --config ./config/webpack/webpack.config.prod.ts",
    "prerelease": "cz bump && cz ch && pnpm build && git add . && git commit --amend --no-edit",
    "type:scss": "tsm src --implementation sass --exportType default",
    "type:scss:watch": "pnpm type:scss -- --watch",
    "data": "superstatic ./data --port 20630 --debug"
  },
